<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="control.project.mapper.LoginMapper">
 	<select id="adminLogin" resultType="control.project.model.AdminLoginVO">
 		select * from admin
 		where adminId = #{adminId} and adminPass = #{adminPass}
 	</select>
</mapper>